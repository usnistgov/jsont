{
    "$schema":  "http://mgi.nist.gov/mgi-json-trans/v0.1",

    "type": "apply",
    "transform": "subjects",

    "transforms": {
        "subject": {
            "type": "xml.element",
            "name": "subject",
            "attrs": { },
            "content": "$in:"
        },
        "subjects": {
            "type": "apply",
            "transform": "newline",
            "input": {
                "type": "apply",
                "transform": {
                    "type": "map",
                    "itemmap": "subject"
                },
                "input": "/content/subject"
            }
        }
    }

}
